#%Module1.0#####################################################################
##
## modules modulefile
##
## modulefiles/modules.  Generated from modules.in by configure.
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "cuda/cuda-10.2 - sets the environment for CUDA 10.2"
}

module-whatis	"cuda/cuda-10.2 - sets the environment for CUDA 10.2"

# for Tcl script use only
set	version		10.2
set	prefix		/usr/local/cuda-${version}

conflict        cuda

setenv		CUDA_HOME	        $prefix
prepend-path    PATH   		        ${prefix}/bin
prepend-path    CUDA_CUDNN_LIBRARY	${prefix}/lib64
prepend-path    LD_LIBRARY_PATH		${prefix}/lib64

